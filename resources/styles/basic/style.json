{
  "version": 8,
  "name": "basic",
  "sources": {
    "openmaptiles": {
      "type": "vector",
      "tiles": ["{toolBaseUrl}/tiles/openmaptiles/{z}/{x}/{y}.pbf"],
      "minzoom": 0,
      "maxzoom": 14,
      "bounds": [-180, -85.0511, 180, 85.0511],
      "attribution": "<a href=\"https://www.maptiler.com/copyright/\" target=\"_blank\">&copy; MapTiler</a> <a href=\"https://www.openstreetmap.org/copyright\" target=\"_blank\">&copy; OpenStreetMap contributors</a>"
    }
  },
  "sprite": "{toolBaseUrl}/sprites/sprites",
  "glyphs": "{toolBaseUrl}/fonts/{fontHash}/{fontstack}/{range}.pbf?fontBaseUrl={fontBaseUrl}",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {
        "background-color": "{colorBackground}"
      }
    },
    {
      "id": "park",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "park",
      "filter": ["==", "$type", "Polygon"],
      "paint": {
        "fill-color": "{colorForest}",
        "fill-opacity": {
          "stops": [
            [5, 0],
            [12, 0.1]
          ]
        }
      }
    },
    {
      "id": "landcover_grass",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landcover",
      "filter": ["all", ["==", "$type", "Polygon"], ["==", "class", "grass"]],
      "paint": {
        "fill-color": "{colorForest}",
        "fill-opacity": {
          "stops": [
            [5, 0],
            [16, 0.25]
          ]
        }
      }
    },
    {
      "id": "landcover_wood",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landcover",
      "minzoom": 5,
      "filter": ["all", ["==", "$type", "Polygon"], ["==", "class", "wood"]],
      "paint": {
        "fill-color": "{colorForest}",
        "fill-opacity": {
          "base": 1,
          "stops": [
            [5, 0],
            [16, 0.25]
          ]
        }
      }
    },
    {
      "id": "waterway",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "waterway",
      "minzoom": 4,
      "maxzoom": 24,
      "filter": ["==", "$type", "LineString"],
      "paint": {
        "line-color": "{colorWater}"
      }
    },
    {
      "id": "water",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "water",
      "filter": ["==", "$type", "Polygon"],
      "paint": {
        "fill-color": "{colorWater}",
        "fill-antialias": true,
        "fill-outline-color": "#e9f5f6"
      }
    },
    {
      "id": "transportation_ferry",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["all", ["in", "class", "ferry"]],
      "paint": {
        "line-color": "{colorWaterway}",
        "line-dasharray": [2, 1]
      }
    },
    {
      "id": "landcover_ice_shelf",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landcover",
      "maxzoom": 8,
      "filter": [
        "all",
        ["==", "$type", "Polygon"],
        ["==", "subclass", "ice_shelf"]
      ],
      "paint": {
        "fill-color": "#ffffff",
        "fill-opacity": 0.5
      }
    },
    {
      "id": "tunnel_motorway_casing",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "minzoom": 6,
      "filter": [
        "all",
        ["==", "$type", "LineString"],
        ["all", ["==", "brunnel", "tunnel"], ["==", "class", "motorway"]]
      ],
      "layout": {
        "line-cap": "butt",
        "line-join": "miter"
      },
      "paint": {
        "line-color": "{colorRoad}",
        "line-width": {
          "base": 1.4,
          "stops": [
            [4, 2],
            [6, 2],
            [20, 30]
          ]
        },
        "line-opacity": 0.75
      }
    },
    {
      "id": "aeroway-taxiway",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "aeroway",
      "minzoom": 12,
      "filter": ["all", ["in", "class", "taxiway"]],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#ffffff",
        "line-width": {
          "base": 1.55,
          "stops": [
            [13, 2],
            [16, 6],
            [20, 20]
          ]
        },
        "line-opacity": 1
      }
    },
    {
      "id": "aeroway-area",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "aeroway",
      "minzoom": 4,
      "filter": [
        "all",
        ["==", "$type", "Polygon"],
        ["in", "class", "runway", "taxiway"]
      ],
      "paint": {
        "fill-opacity": {
          "base": 1,
          "stops": [
            [13, 0],
            [14, 1]
          ]
        },
        "fill-color": "{colorBuilding}"
      }
    },
    {
      "id": "aeroway-runway",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "aeroway",
      "minzoom": 11,
      "filter": [
        "all",
        ["in", "class", "runway"],
        ["==", "$type", "LineString"]
      ],
      "layout": {
        "line-cap": "square",
        "line-join": "round"
      },
      "paint": {
        "line-color": "{colorBuilding}",
        "line-width": {
          "base": 1.5,
          "stops": [
            [11, 4],
            [17, 50]
          ]
        },
        "line-opacity": 1
      }
    },
    {
      "id": "road_area_pier",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "minzoom": 0,
      "filter": ["all", ["==", "$type", "Polygon"], ["==", "class", "pier"]],
      "paint": {
        "fill-color": "#ececf0",
        "fill-antialias": true
      }
    },
    {
      "id": "road_pier",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "minzoom": 5,
      "filter": ["all", ["==", "$type", "LineString"], ["in", "class", "pier"]],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#e3e4e9",
        "line-width": {
          "base": 1.2,
          "stops": [
            [15, 1],
            [17, 4]
          ]
        }
      }
    },
    {
      "id": "highway_path",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "minzoom": 6.5,
      "filter": ["all", ["==", "$type", "LineString"], ["==", "class", "path"]],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "{colorRoad}",
        "line-width": {
          "base": 1.2,
          "stops": [
            [13, 0.25],
            [20, 5]
          ]
        },
        "line-opacity": 1
      }
    },
    {
      "id": "highway_minor",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "minzoom": 6.5,
      "filter": [
        "all",
        ["==", "$type", "LineString"],
        ["in", "class", "minor", "service", "track"]
      ],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "{colorRoad}",
        "line-width": {
          "base": 1.55,
          "stops": [
            [13, 1],
            [20, 20]
          ]
        },
        "line-opacity": 0.9
      }
    },
    {
      "id": "highway_major",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "minzoom": 6.5,
      "filter": [
        "all",
        ["==", "$type", "LineString"],
        ["in", "class", "primary", "secondary", "tertiary", "trunk"]
      ],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "{colorRoad}",
        "line-width": {
          "base": 1.3,
          "stops": [
            [10, 1],
            [20, 16]
          ]
        }
      }
    },
    {
      "id": "highway_major_subtle",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "minzoom": 6.5,
      "maxzoom": 11,
      "filter": [
        "all",
        ["==", "$type", "LineString"],
        ["in", "class", "primary", "secondary", "tertiary", "trunk"]
      ],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "{colorRoad}",
        "line-width": {
          "stops": [
            [6, 0.6],
            [10, 1]
          ]
        }
      }
    },
    {
      "id": "highway_motorway_inner",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "minzoom": 6.5,
      "filter": [
        "all",
        ["==", "$type", "LineString"],
        [
          "all",
          ["!in", "brunnel", "bridge", "tunnel"],
          ["==", "class", "motorway"]
        ]
      ],
      "layout": {
        "line-cap": "square",
        "line-join": "round"
      },
      "paint": {
        "line-color": "{colorRoad}",
        "line-width": {
          "base": 1.4,
          "stops": [
            [4, 1],
            [5, 1.5],
            [20, 18]
          ]
        }
      }
    },
    {
      "id": "railway_transit",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "minzoom": 5,
      "maxzoom": 24,
      "filter": [
        "all",
        ["==", "$type", "LineString"],
        ["all", ["==", "class", "transit"], ["!in", "brunnel", "tunnel"]]
      ],
      "layout": {
        "line-join": "round"
      },
      "paint": {
        "line-color": "{colorRailway}",
        "line-width": 1
      }
    },
    {
      "id": "railway_service",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "minzoom": 5,
      "filter": [
        "all",
        ["==", "$type", "LineString"],
        ["all", ["==", "class", "rail"], ["has", "service"]]
      ],
      "layout": {
        "line-join": "round"
      },
      "paint": {
        "line-color": "{colorRailway}",
        "line-width": 1
      }
    },
    {
      "id": "railway",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "minzoom": 5,
      "maxzoom": 24,
      "filter": [
        "all",
        ["==", "$type", "LineString"],
        ["all", ["!has", "service"], ["==", "class", "rail"]]
      ],
      "layout": {
        "line-join": "round"
      },
      "paint": {
        "line-color": "{colorRailway}",
        "line-width": {
          "base": 1.3,
          "stops": [
            [16, 1],
            [20, 6]
          ]
        }
      }
    },
    {
      "id": "building-walls",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "building",
      "minzoom": 14,
      "paint": {
        "fill-color": "{colorBackground}",
        "fill-outline-color": "#ffffff",
        "fill-antialias": true,
        "fill-translate": [2, 1]
      }
    },
    {
      "id": "building",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "building",
      "minzoom": 14,
      "paint": {
        "fill-color": "{colorBuilding}",
        "fill-outline-color": "{colorBackground}"
      }
    },
    {
      "id": "highway_motorway_bridge_casing",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "minzoom": 6.5,
      "filter": [
        "all",
        ["==", "$type", "LineString"],
        ["all", ["==", "brunnel", "bridge"], ["==", "class", "motorway"]]
      ],
      "layout": {
        "line-cap": "butt",
        "line-join": "miter"
      },
      "paint": {
        "line-color": "{colorRoad}",
        "line-width": {
          "base": 1.4,
          "stops": [
            [5.8, 0],
            [6, 4],
            [20, 35]
          ]
        },
        "line-dasharray": [2, 0],
        "line-opacity": 1
      }
    },
    {
      "id": "highway_motorway_bridge_inner",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "minzoom": 6.5,
      "filter": [
        "all",
        ["==", "$type", "LineString"],
        ["all", ["==", "brunnel", "bridge"], ["==", "class", "motorway"]]
      ],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "{colorRoad}",
        "line-width": {
          "base": 1.4,
          "stops": [
            [4, 2],
            [6, 1.3],
            [20, 30]
          ]
        }
      }
    },
    {
      "id": "highway_name_other",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "transportation_name",
      "filter": [
        "all",
        ["!=", "class", "motorway"],
        ["==", "$type", "LineString"],
        ["!has", "name:de"]
      ],
      "layout": {
        "text-size": 10,
        "text-max-angle": 30,
        "text-transform": "none",
        "symbol-spacing": 350,
        "text-font": ["{fontSansRegular}"],
        "symbol-placement": "line",
        "text-rotation-alignment": "map",
        "text-pitch-alignment": "viewport",
        "text-field": "{name:latin}"
      },
      "paint": {
        "text-color": "{colorRoadText}",
        "text-halo-color": "{colorBackground}",
        "text-halo-width": "{textHaloWidth}",
        "text-halo-blur": "{textHaloWidth}"
      }
    },
    {
      "id": "highway_name_other-de",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "transportation_name",
      "filter": [
        "all",
        ["!=", "class", "motorway"],
        ["==", "$type", "LineString"],
        ["has", "name:de"]
      ],
      "layout": {
        "text-size": 10,
        "text-max-angle": 30,
        "text-transform": "uppercase",
        "symbol-spacing": 350,
        "text-font": ["{fontSansRegular}"],
        "symbol-placement": "line",
        "text-rotation-alignment": "map",
        "text-pitch-alignment": "viewport",
        "text-field": "{name:de}"
      },
      "paint": {
        "text-color": "{colorRoadText}",
        "text-halo-color": "{colorBackground}",
        "text-halo-width": "{textHaloWidth}",
        "text-halo-blur": "{textHaloWidth}"
      }
    },
    {
      "id": "boundary_community",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "boundary",
      "minzoom": 9,
      "maxzoom": 24,
      "filter": ["all", ["==", "admin_level", 6], ["==", "maritime", 0]],
      "layout": {
        "line-cap": "square",
        "line-join": "bevel"
      },
      "paint": {
        "line-color": "{colorBoundaryCommunity}",
        "line-width": {
          "stops": [
            [4, 0.1],
            [6, 0.3],
            [10, 0.9]
          ]
        },
        "line-blur": 0,
        "line-opacity": 1
      }
    },
    {
      "id": "boundary_state",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "boundary",
      "minzoom": 3,
      "maxzoom": 24,
      "filter": ["all", ["==", "admin_level", 4], ["==", "maritime", 0]],
      "layout": {
        "line-cap": "square",
        "line-join": "bevel"
      },
      "paint": {
        "line-color": "{colorBoundaryState}",
        "line-width": {
          "stops": [
            [4, 0.4],
            [10, 1.5]
          ]
        },
        "line-blur": 0,
        "line-opacity": 1
      }
    },
    {
      "id": "boundary_country",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "boundary",
      "filter": [
        "all",
        ["==", "admin_level", 2],
        ["==", "disputed", 0],
        ["==", "maritime", 0]
      ],
      "layout": {
        "line-cap": "square",
        "line-join": "bevel"
      },
      "paint": {
        "line-color": "{colorBoundaryCountry}",
        "line-width": {
          "stops": [
            [0, 0.5],
            [10, 1.5]
          ]
        },
        "line-blur": 0,
        "line-opacity": 1
      }
    },
    {
      "id": "boundary_country_disputed",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "boundary",
      "filter": ["all", ["==", "admin_level", 2], ["==", "disputed", 1]],
      "layout": {
        "line-cap": "square",
        "line-join": "bevel"
      },
      "paint": {
        "line-color": "{colorBoundaryCountry}",
        "line-width": {
          "stops": [
            [0, 0.5],
            [10, 1.5]
          ]
        },
        "line-blur": 0,
        "line-opacity": 1,
        "line-dasharray": [0.5, 2, 2]
      }
    },
    {
      "id": "place_suburb-de",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "place",
      "minzoom": 5,
      "maxzoom": 15,
      "filter": [
        "all",
        ["==", "$type", "Point"],
        ["==", "class", "suburb"],
        ["has", "name:de"]
      ],
      "layout": {
        "text-size": "{textSizeCity}",
        "text-font": ["{fontSansRegular}"],

        "text-field": "{name:de}"
      },
      "paint": {
        "text-color": "{colorText}",
        "text-halo-color": "{colorBackground}",
        "text-halo-width": "{textHaloWidth}",
        "text-halo-blur": "{textHaloWidth}"
      }
    },
    {
      "id": "place_village",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "place",
      "minzoom": 5,
      "maxzoom": 14,
      "filter": [
        "all",
        ["==", "$type", "Point"],
        ["==", "class", "village"],
        ["!has", "name:de"]
      ],
      "layout": {
        "text-size": "{textSizeCity}",
        "text-font": ["{fontSansRegular}"],
        "text-field": "{name:latin}"
      },
      "paint": {
        "text-color": "{colorText}",
        "text-halo-color": "{colorBackground}",
        "text-halo-width": "{textHaloWidth}",
        "text-halo-blur": "{textHaloWidth}"
      }
    },
    {
      "id": "place_village-de",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "place",
      "maxzoom": 14,
      "filter": [
        "all",
        ["==", "$type", "Point"],
        ["==", "class", "village"],
        ["has", "name:de"]
      ],
      "layout": {
        "text-size": "{textSizeCity}",
        "text-font": ["{fontSansRegular}"],
        "text-field": "{name:de}"
      },
      "paint": {
        "text-color": "{colorText}",
        "text-halo-color": "{colorBackground}",
        "text-halo-width": "{textHaloWidth}",
        "text-halo-blur": "{textHaloWidth}"
      }
    },
    {
      "id": "place_town-de",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "place",
      "maxzoom": 15,
      "filter": [
        "all",
        ["==", "$type", "Point"],
        ["==", "class", "town"],
        ["has", "name:de"]
      ],
      "layout": {
        "text-size": "{textSizeCity}",
        "text-font": ["{fontSansRegular}"],
        "text-field": "{name:de}"
      },
      "paint": {
        "text-color": "{colorText}",
        "text-halo-color": "{colorBackground}",
        "text-halo-width": "{textHaloWidth}",
        "text-halo-blur": "{textHaloWidth}"
      }
    },
    {
      "id": "place_city-de",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "place",
      "minzoom": 5,
      "maxzoom": 14,
      "filter": [
        "all",
        ["==", "$type", "Point"],
        [
          "all",
          ["!=", "capital", 2],
          ["==", "class", "city"],
          [">", "rank", 3]
        ],
        ["has", "name:de"]
      ],
      "layout": {
        "text-size": "{textSizeCity}",
        "text-font": ["{fontSansRegular}"],
        "text-field": "{name:de}"
      },
      "paint": {
        "text-color": "{colorText}",
        "text-halo-color": "{colorBackground}",
        "text-halo-width": "{textHaloWidth}",
        "text-halo-blur": "{textHaloWidth}"
      }
    },
    {
      "id": "place_capital-de",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "place",
      "minzoom": 5,
      "maxzoom": 12,
      "filter": [
        "all",
        ["==", "$type", "Point"],
        ["all", ["==", "capital", 2], ["==", "class", "city"]],
        ["has", "name:de"]
      ],
      "layout": {
        "text-size": "{textSizeCapital}",
        "text-font": ["{fontSansMedium}"],
        "text-field": "{name:de}"
      },
      "paint": {
        "text-color": "{colorText}",
        "text-halo-color": "{colorBackground}",
        "text-halo-width": "{textHaloWidth}",
        "text-halo-blur": "{textHaloWidth}"
      }
    },
    {
      "id": "place_city_large-de",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "place",
      "minzoom": 5,
      "maxzoom": 12,
      "filter": [
        "all",
        ["==", "$type", "Point"],
        [
          "all",
          ["!=", "capital", 2],
          ["<=", "rank", 3],
          ["==", "class", "city"]
        ],
        ["has", "name:de"]
      ],
      "layout": {
